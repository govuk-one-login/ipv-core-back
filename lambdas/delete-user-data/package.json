{
  "name": "delete-user-data",
  "version": "1.0.0",
  "description": "Delete user data TS node lambda",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --verbose",
    "build": "sam build -t ../../deploy-delete-user-data/template.yaml",
    "local-invoke": "sam local invoke -e local-dev/sample-sqs-event.json -n local-dev/local-env.json"
  },
  "devDependencies": {
    "@aws-lambda-powertools/commons": "1.17.0",
    "@aws-sdk/client-dynamodb": "3.485.0",
    "@aws-sdk/client-sns": "3.485.0",
    "@aws-sdk/client-sqs": "3.485.0",
    "@aws-sdk/client-ssm": "3.485.0",
    "@aws-sdk/lib-dynamodb": "3.485.0",
    "@swc/jest": "0.2.24",
    "@types/aws-lambda": "8.10.109",
    "@types/jest": "29.5.8",
    "@types/node": "18.19.3",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-prettier": "5.1.2",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "typescript": "5.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "@swc/jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.js"
    ]
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.17.0"
  }
}
