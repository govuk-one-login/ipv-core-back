{
  "name": "delete-user-data",
  "version": "1.0.0",
  "description": "Delete user data TS node lambda",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --verbose",
    "build": "sam build -t ../../deploy-delete-user-data/template.yaml",
    "local-invoke": "sam local invoke -e local-dev/sample-sqs-event.json -n local-dev/local-env.json"
  },
  "devDependencies": {
    "@aws-lambda-powertools/commons": "1.14.2",
    "@aws-sdk/client-dynamodb": "3.445.0",
    "@aws-sdk/client-sns": "3.391.0",
    "@aws-sdk/client-sqs": "3.391.0",
    "@aws-sdk/client-ssm": "3.391.0",
    "@aws-sdk/lib-dynamodb": "3.441.0",
    "@swc/jest": "0.2.24",
    "@types/aws-lambda": "8.10.109",
    "@types/jest": "29.2.6",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.4.3",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.3.1",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "@swc/jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.js"
    ]
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.12.1"
  }
}
