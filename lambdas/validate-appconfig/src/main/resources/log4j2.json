{
  "Configuration": {
    "status": "warn",
    "appenders": {
      "Console": {
        "name": "JsonAppender",
        "target": "SYSTEM_OUT",
        "JsonTemplateLayout": {
          "eventTemplateUri": "classpath:IpvLambdaJsonLayout.json"
        }
      }
    },
    "Loggers": {
      "Root": {
        "level": "info",
        "AppenderRef": {
          "ref": "JsonAppender"
        }
      }
    }
  }
}
